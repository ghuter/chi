import std;

toString :: fun (pts: ^Point) String {
	str := newString();
	sprintf(str.ch, "Point {x: %d, y:%d}", pts.x, pts.y);
	return str;
}

Point :: struct {
	x: i32;
	y: i32;
}

String :: struct {
	ch: ^u8;
	sz: u32;
}

pow2 :: fun() i8 { return 1; }

helloX :: fun (x: i32) {	
	for i := 0; i < x; i += 1 {
		puts("Hello !");
	}
}
